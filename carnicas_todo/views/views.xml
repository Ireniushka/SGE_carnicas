<odoo>
    <record id="view_tree_carnica" model="ir.ui.view">
        <field name="name">view.tree.todo</field>
        <field name="model">animal.CarnicasModule</field>
        <field name="arch" type="xml">
            <tree>
                <field name="id"/>
                <field name="Raza"/>
                <field name="Edad"/>
                <field name="Matadero"/>
                <field name="Fecha_sacr"/>
            </tree>
        </field>
    </record>

    <record id="action_view_todo" model="ir.action.act_window">
        <field name="name">Lista de reses</field>
        <field name="res_model">animal.CarnicasModule</field>
        <field name="view_mode">tree, form</field>
    </record>

    <menuitem id="menu_todo" parent="" action="carnicas_todo.action_view_todo"/>
</odoo>